    "specs_hotspots_EpDANH": {
      "type": "specs-hotspots",
      "blocks": {
        "hotspot_JmxJWP": {
          "type": "hotspot",
          "settings": {
            "title": "High performance aluminium",
            "text": "Our aluminium is specially formulated to resist denting, ensuring everything inside the box is kept safe! Our aluminium also has a high toughness (meaning it can take a real beating before it breaks). To our knowledge, no other box on the market uses an aluminium shell.",
            "pos_x": 45,
            "pos_y": 32,
            "placement": "top-left"
          }
        },
        "hotspot_kUHwqk": {
          "type": "hotspot",
          "settings": {
            "title": "Flattened edges",
            "text": "Most boxes on the market have a relatively thin, sharp edge on the hard part. During an impact, this thin and sharp edge concentrates the impact into a smaller area, making it hurt more! We use a large, flanged edge on the rigid component of our box, spreading the load and making sure impacts hurt a lot less - keeping you in the zone.",
            "pos_x": 65,
            "pos_y": 32,
            "placement": "top-right"
          }
        },
        "hotspot_mqBApf": {
          "type": "hotspot",
          "settings": {
            "title": "Shock absorbing dual foam",
            "text": "We use two separate layers of foam, one harder and one softer, which our R&D has shown improves player comfort (both in normal use, and during impacts). Both foams are an advanced polyurethane compound for improved shock absorption compared to typical foams used in sports gear.",
            "pos_x": 35,
            "pos_y": 70,
            "placement": "bottom-left"
          }
        },
        "hotspot_g8fpFt": {
          "type": "hotspot",
          "settings": {
            "title": "UNIQUE SHAPE",
            "text": "We took an ergonomic focus to shape design. Comfort is equally as important as protection which is why we created a shape that minimises obstruction for movement and chafe risk.",
            "pos_x": 48,
            "pos_y": 54,
            "placement": "bottom-right"
          }
        }
      },
      "block_order": [
        "hotspot_JmxJWP",
        "hotspot_kUHwqk",
        "hotspot_mqBApf",
        "hotspot_g8fpFt"
      ],
