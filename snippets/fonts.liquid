{% # theme-check-disable %}
<link rel="preload" as="font" href="{{ 'PPMonumentExtended-Light.woff2' | asset_url }}" type="font/woff2" crossorigin>

<style>
@font-face {
  font-family: 'PP Monument Extended';
  src: url('{{ "PPMonumentExtended-Light.woff2" | asset_url }}') format('woff2'),
       url('{{ "PPMonumentExtended-Light.woff"  | asset_url }}') format('woff');
  font-style: normal;
  font-weight: 300;
  font-display: swap;
}
@font-face {
          font-family: 'Graphik';
          src: url('{{ "Graphik-Light.woff2" | asset_url }}') format('woff2'),
            url('{{ "Graphik-Light.woff" | asset_url }}') format('woff');
          font-weight: 300;
          font-style: normal;
          font-display: swap;
      }
      @font-face {
          font-family: 'Graphik';
          src: url('{{ "Graphik-Regular.woff2" | asset_url }}') format('woff2'),
            url('{{ "Graphik-Regular.woff" | asset_url }}') format('woff');
          font-weight: normal;
          font-style: normal;
          font-display: swap;
      }
      @font-face {
          font-family: 'Graphik';
          src: url('{{ "Graphik-Semibold.woff2" | asset_url }}') format('woff2'),
            url('{{ "Graphik-Semibold.woff" | asset_url }}') format('woff');
          font-weight: 600;
          font-style: normal;
          font-display: swap;
      }
</style>

{%- unless settings.type_body_font.system? -%}
  {% comment %} <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  > {% endcomment %}
  <link rel="preload" as="font" href="{{ 'Graphik-Regular.woff2' | asset_url }}" type="font/woff2" crossorigin>
{%- endunless -%}
{%- unless settings.type_subheading_font.system? -%}
  {% comment %} <link
    rel="preload"
    as="font"
    href="{{ settings.type_subheading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  > {% endcomment %}
  <link rel="preload" as="font" href="{{ 'Graphik-Light.woff2' | asset_url }}" type="font/woff2" crossorigin>
{%- endunless -%}
{%- unless settings.type_heading_font.system? -%}
  {% comment %} <link
    rel="preload"
    as="font"
    href="{{ settings.type_heading_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  > {% endcomment %}
{%- endunless -%}
{%- unless settings.type_accent_font.system? -%}
  {% comment %} <link
    rel="preload"
    as="font"
    href="{{ settings.type_accent_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  > {% endcomment %}
{%- endunless -%}
{% # theme-check-enable %}
